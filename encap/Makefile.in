# @configure_input@
include ../include/config.Makefile
VPATH = @srcdir@

install ::

Makefile : Makefile.in; cd ..; ./config.status encap/Makefile

ifeq "$(encap)" "yes"

install ::
	[ "$(prefix)" != /usr ] 
	[ "$(prefix)" != /usr/local ] 

install :: $(prefix)/encapinfo
$(prefix)/encapinfo: encapinfo
	$(INSTALL_DATA) $< "$@"

install :: $(prefix)/postinstall
$(prefix)/postinstall: postinstall
	$(INSTALL_PROGRAM) $< "$@"
postinstall : @srcdir@ ; cd .. && ./config.status encap/$@

install :: $(prefix)/INSTALL

$(prefix)/INSTALL : INSTALL
	cp $< $@

endif

