# @configure_input@
# DOTS is the path to M2/Macaulay2 from this directory

DEFERRED = yes
DOTS = ../..
include $(DOTS)/../include/config.Makefile
VPATH = @srcdir@

ifeq "@DEBUG@" "no"
MLIMIT = 400000
VLIMIT = 400000
TLIMIT = 450
else
# give the debug version more time and space
MLIMIT = 600000
VLIMIT = 600000
TLIMIT = 600
endif

SRCDIR = @srcdir@
include ../Makefile.test

Makefile: Makefile.in; cd $(DOTS)/..; ./config.status Macaulay2/tests/goals/Makefile

# Local Variables:
# compile-command: "make -k -C $M2BUILDDIR/Macaulay2/packages/Macaulay2Doc/test/slow "
# End:
.NOTPARALLEL:
