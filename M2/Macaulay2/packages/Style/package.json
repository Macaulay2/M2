{
  "scripts": {
    "postinstall": "rm -rf katex && copy-files-from-to"
  },
  "dependencies": {
    "copy-files-from-to": "^3.9.1",
    "katex": "^0.16.10"
  },
  "copyFiles": [
    {
      "from": "node_modules/katex/dist/contrib/auto-render.min.js",
      "to": "katex/contrib/"
    },
    {
      "from": "node_modules/katex/dist/contrib/copy-tex.min.js",
      "to": "katex/contrib/"
    },
    {
      "from": "node_modules/katex/dist/contrib/render-a11y-string.min.js",
      "to": "katex/contrib/"
    },
    {
      "from": "node_modules/katex/dist/fonts",
      "to": "katex/"
    },
    {
      "from": "node_modules/katex/dist/katex.min.*",
      "to": "katex/"
    },
    {
      "from": "node_modules/katex/LICENSE",
      "to": "katex/"
    }
  ],
  "copyFilesSettings": {
    "whenFileExists": "overwrite"
  }
}
