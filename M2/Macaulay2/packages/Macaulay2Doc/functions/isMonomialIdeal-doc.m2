document { 
     Key => {isMonomialIdeal, (isMonomialIdeal,Ideal), (isMonomialIdeal,Thing), (isMonomialIdeal, Module), (isMonomialIdeal,MonomialIdeal)},
     Headline => "whether something is a monomial ideal",
     Usage => "isMonomialIdeal I",
     Inputs => {
	  "I" => Thing
	  },
     Outputs => {
	  Boolean => {TO "true", " if ", TT "I", " is either a ", TO2("MonomialIdeal", "monomial ideal"), 
	       " or an ", TO2("Ideal", "ideal"), " generated by monomials and ", TO "false", " otherwise"}
	  },
     EXAMPLE {
	  "QQ[w,x,y,z];",
	  "I = minors(2, matrix{{w,x,y},{x,y,z}})",
	  "isMonomialIdeal I",
	  "J = ideal leadTerm I",
	  "isMonomialIdeal J",
	  "K = monomialIdeal I",
	  "isMonomialIdeal K"
	  },
     SeeAlso => {monomialIdeal}
     }

