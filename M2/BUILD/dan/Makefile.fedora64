# -*- makefile-gmake -*-
DEBUG = no
ifeq ($(DEBUG),yes)
DIRECTORY = fedora64.debug
CONFIGOPTIONS += --enable-debug
INSTALLOPTIONS += prefix=/none
else
DIRECTORY = fedora64
CONFIGOPTIONS += --enable-rpm --disable-common-staging-area
endif
include Makefile.include
default: install check
