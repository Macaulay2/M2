# -*- makefile-gmake -*-

CONFIGOPTIONS += --enable-debug --disable-optimize
# CONFIGOPTIONS += --with-python
CC=icc
CXX=icpc
DIRECTORY = ubuntu64-debug-icc
include Makefile.include
default: check
arch-chk:
	grep -q Ubuntu /etc/issue
	[ `uname -m` = x86_64 ]
