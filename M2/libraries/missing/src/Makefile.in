VPATH = @srcdir@
include ../../../include/config.Makefile
all: libmissing.a
libmissing.a : missing.o
	ar -r $@ $^
	@RANLIB@ $@
install:
	$(MKDIR_P) $(prefix)/lib
	@INSTALL_DATA@ libmissing.a $(prefix)/lib
check:
clean:; rm -f *.o *.a
Makefile: @srcdir@/Makefile.in ; cd ../../.. && ./config.status libraries/missing/src/Makefile
