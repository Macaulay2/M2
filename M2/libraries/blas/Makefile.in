# @configure_input@
VPATH = @srcdir@
include ../../include/config.Makefile
# http://www.netlib.org/blas/
# Basic Linear Algebra Subprograms
URL = http://www.netlib.org/blas
TARFILE = blas.tgz
TARDIR = BLAS
CONFIGURECMD = :
BUILDOPTIONS = BLASLIB=libf77blas.a
INSTALLDIRS = lib
INSTALLCMD = install -d $(PREFIX)/lib && install $(BUILDDIR)/libf77blas.a $(PREFIX)/lib/libf77blas.a
include @srcdir@/../Makefile.library
Makefile : Makefile.in ; cd ../.. && ./config.status libraries/blas/Makefile
