# http://www.netlib.org/blas/
# Basic Linear Algebra Subprograms
URL = http://www.netlib.org/blas
TARFILE = blas.tgz
TARDIR = BLAS
CONFIGURECMD = :
BUILDOPTIONS = BLASLIB=libf77blas.a
INSTALLDIRS = lib
INSTALLCMD = install -d $(PREFIX)/$(libm2RelDir)/lib && install libf77blas.a $(PREFIX)/$(libm2RelDir)/lib/libf77blas.a
include ../Makefile.library
Makefile : @srcdir@/Makefile.in ; cd ../.. && ./config.status libraries/blas/Makefile
