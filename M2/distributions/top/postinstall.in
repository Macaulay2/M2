#! /bin/sh -e
# we want to ignore the --datarootdir setting, whatever that means, so here we we mention it: @datarootdir@
pre_prefix=$ENCAP_SOURCE/$ENCAP_PKGNAME
prefix=$ENCAP_TARGET
exec_prefix=$prefix
cd "@pre_infodir@"
for i in *.info
do (set -x ; install-info --quiet --info-dir="@infodir@" "$i")
done
