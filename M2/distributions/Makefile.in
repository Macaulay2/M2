# @configure_input@
include ../include/config.Makefile
VPATH = @srcdir@

all:
	$(MAKE) -C top $@
ifeq "@M2TARFILE@" "yes"
	$(MAKE) -C tar all
endif
ifeq "@DEB@" "yes"
	$(MAKE) -C deb all
endif
ifeq "@RPM@" "yes"
	$(MAKE) -C rpm all
endif
ifeq "@DMG@" "yes"
	$(MAKE) -C dmg all
endif
ifeq "@CYGWIN@" "yes"
	$(MAKE) -C cygwin all
endif

install:
	$(MAKE) -C top $@
	$(MAKE) -C install $@

check:
	$(MAKE) -C deb $@
clean distclean:
	$(MAKE) -C dmg $@
	$(MAKE) -C rpm $@
	$(MAKE) -C deb $@
	$(MAKE) -C cygwin $@
	$(MAKE) -C tar $@
	$(MAKE) -C top $@
	$(MAKE) -C install $@
Makefile : Makefile.in; cd ..; ./config.status distributions/Makefile
