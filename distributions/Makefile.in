# @configure_input@
include ../include/config.Makefile
VPATH = @srcdir@

install pre-install all ::; $(MAKE) -C top $@

ifeq "@M2TARFILE@" "yes"
pre-install ::; $(MAKE) -C tar pre-install
endif
ifeq "@DEB@" "yes"
pre-install ::; $(MAKE) -C deb pre-install
endif
ifeq "@RPM@" "yes"
pre-install ::; $(MAKE) -C rpm pre-install
endif
ifeq "@DMG@" "yes"
pre-install ::; $(MAKE) -C dmg pre-install
endif
ifeq "@CYGWIN@" "yes"
pre-install ::; $(MAKE) -C cygwin pre-install
endif
check ::
	$(MAKE) -C deb $@
clean distclean ::
	$(MAKE) -C dmg $@
	$(MAKE) -C rpm $@
	$(MAKE) -C deb $@
	$(MAKE) -C cygwin $@
	$(MAKE) -C tar $@

uninstall install pre-install distclean ::; $(MAKE) -C install $@

Makefile : Makefile.in; cd ..; ./config.status distributions/Makefile
